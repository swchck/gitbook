{
    "title": "Developer Interview Questions",
    "author": "Andrew Cohen",
    "description": "Definitive guide to Software developer interview",
    "plugins": [
        "custom-favicon",
        "github",
        "-sharing",
        "autocover",
        "hide-element",
        "back-to-top-button",
        "youtube",
        "collapsible-menu",
        "validate-links",
        "gist",
        "hints",
        "sitemap"
    ],
    "pluginsConfig": {
        "favicon": "favicon.ico",
        "github": {
            "url": "https://github.com/swchck"
        },
        "hide-element": {
            "elements": [
                ".gitbook-link"
            ]
        },
        "autocover": {
            "font": {
                "size": null,
                "family": "Impact",
                "color": "#FFF"
            },
            "size": {
                "w": 1800,
                "h": 2360
            },
            "background": {
                "color": "#09F"
            }
        },
        "hints": {
            "info": "fa fa-info-circle",
            "tip": "fa fa-mortar-board",
            "danger": "fa fa-exclamation-cicle",
            "working": "fa fa-wrench"
        },
        "sitemap": {
            "hostname": "https://swchck.github.io/iddqd/"
        }
    }
}